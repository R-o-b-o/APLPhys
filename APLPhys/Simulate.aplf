 Simulate nsteps;i
 (acc ene_pot_avg virial)←ComputeForces pos
 :If 0=≢dumps
     :For i :In ⍳nsteps
         log←ComputeForces RunStyle pos vel acc
         (pos vel acc)←3↑log
     :EndFor
 :Else
     :For i :In ⍳nsteps
         log←ComputeForces RunStyle pos vel acc ⍬ ⍬ ⍬ ⍬
         (pos vel acc)←3↑log
         :If 0<≢d←⍸0=dumpfreqs|i
             (⊂dumpfiles[d])DumpText i,⊂⍎¨dumps[d]
         :EndIf
     :EndFor
 :EndIf
